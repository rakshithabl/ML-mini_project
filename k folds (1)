{"nbformat":4,"nbformat_minor":0,"metadata":{"colab":{"provenance":[],"authorship_tag":"ABX9TyM9UvBmaWLvxZOjA/x8FzGS"},"kernelspec":{"name":"python3","display_name":"Python 3"},"language_info":{"name":"python"}},"cells":[{"cell_type":"code","source":["import pandas as pd\n","df = pd.read_csv('spam_emails_dataset.csv')\n","df.head()\n","from sklearn.model_selection import KFold\n","from sklearn.linear_model import LogisticRegression\n","from sklearn.metrics import accuracy_score\n","import numpy as np\n","\n","X = df.drop('spam', axis=1)\n","y = df['spam']\n","\n","kf = KFold(n_splits=5, shuffle=True, random_state=42)\n","model = LogisticRegression()\n","scores = []\n","fold = 1\n","for train_idx, test_idx in kf.split(X):\n","    X_train, X_test = X.iloc[train_idx], X.iloc[test_idx]\n","    y_train, y_test = y.iloc[train_idx], y.iloc[test_idx]\n","\n","    model.fit(X_train, y_train)\n","    preds = model.predict(X_test)\n","    acc = accuracy_score(y_test, preds)\n","    scores.append(acc)\n","    print(f\"Fold {fold}: Accuracy = {acc:.2f}\")\n","    fold += 1\n","\n","print(\"\\nAverage Accuracy:\", round(np.mean(scores), 2))\n","import matplotlib.pyplot as plt\n","plt.plot(range(1, 6), scores, marker='o')\n","plt.title(\"K-Fold Accuracy per Fold\")\n","plt.xlabel(\"Fold\")\n","plt.ylabel(\"Accuracy\")\n","plt.show()\n","\n","\n","\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":593},"id":"IigF63aiFSJv","executionInfo":{"status":"ok","timestamp":1761559691146,"user_tz":-330,"elapsed":565,"user":{"displayName":"Rakshitha BL","userId":"04543297468823917835"}},"outputId":"d94e1fcd-b8e2-4508-90aa-1601fe5a2f1f"},"execution_count":9,"outputs":[{"output_type":"stream","name":"stdout","text":["Fold 1: Accuracy = 0.97\n","Fold 2: Accuracy = 1.00\n","Fold 3: Accuracy = 0.90\n","Fold 4: Accuracy = 0.97\n","Fold 5: Accuracy = 0.97\n","\n","Average Accuracy: 0.96\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"]